{
  "name": "make-3d-printable-yoyo",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "lint": "eslint --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint-fix": "eslint --fix --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "file:lint": "ls-lint",
    "lint:config": "eslint --print-config src/index.ts > .eslintprint.json",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "dep": "depcruise app --include-only \"^app\" --output-type dot | dot -T svg > dependency-graph.svg",
    "dep:model": "depcruise app --include-only \"^app/model\" --output-type dot | dot -T svg > dependency-graph.svg",
    "knip": "knip",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "npx vitest --coverage.enabled --coverage.all",
    "test:ui": "vitest --ui --coverage"
  },
  "dependencies": {
    "@ls-lint/ls-lint": "^2.3.0",
    "@react-router/node": "^7.3.0",
    "@react-router/serve": "^7.3.0",
    "@react-three/drei": "^10.0.4",
    "@react-three/fiber": "^9.1.0",
    "@vercel/react-router": "^1.1.0",
    "immer": "^10.1.1",
    "isbot": "^5.1.23",
    "konva": "^9.3.18",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-konva": "^19.0.3",
    "react-router": "^7.3.0",
    "three": "^0.180.0",
    "three-csg-ts": "^3.2.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@react-router/dev": "^7.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dependency-cruiser": "^17.0.0",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.15.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^6.0.0",
    "globals": "^16.0.0",
    "happy-dom": "^19.0.0",
    "knip": "^5.57.2",
    "sass": "^1.85.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
