    "@remix-run/serve": "^2.9.2",
    "@vercel/remix": "^2.11.1",
    "canvas": "^3.1.0",
    "isbot": "^4.1.0",
    "konva": "^9.3.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "three": "^0.164.1",
    "three-csg-ts": "^3.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
